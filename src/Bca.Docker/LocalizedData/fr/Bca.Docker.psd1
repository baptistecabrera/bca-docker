@{
    Global       = @{
        Debug = @{
            Entering = "Début de la fonction « {0} »"
            Leaving  = "Fin de la fonction « {0} »"
        }
    }

    ImportModule = @{
        Error = @{
            ImportError = @{
                Message = "Impossible d'importer la fonction « {0} »  : {1}"
                Target  = "Fonction"
            }
        }
    }
}